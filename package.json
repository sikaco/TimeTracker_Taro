{
  "name": "time-tracker",
  "version": "1.0.0",
  "private": true,
  "description": "a self-used time tracker",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:rn-ios": "taro build --type rn --platform ios",
    "build:rn-android": "taro build --type rn --platform android",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch --qr",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "publish:pages": "yarn build:h5 && yarn gh-pages -d dist/h5 -t"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "qiuz",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@codler/react-native-keyboard-aware-scroll-view": "^2.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@reduxjs/toolkit": "^1.8.3",
    "@tarojs/cli": "3.5.4",
    "@tarojs/components": "3.5.4",
    "@tarojs/plugin-framework-react": "3.5.4",
    "@tarojs/react": "3.5.4",
    "@tarojs/rn-runner": "3.5.4",
    "@tarojs/runtime": "3.5.4",
    "@tarojs/taro": "3.5.4",
    "@tarojs/taro-rn": "3.5.4",
    "classnames": "^2.3.1",
    "jasmine": "^4.2.1",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "observable-hooks": "^4.2.0",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.68.2",
    "react-redux": "^7.2.0",
    "react-refresh": "^0.14.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.1.6",
    "rxjs": "^7.5.5",
    "taro-ui": "^3.0.0-alpha.3",
    "webpack": "5.69.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@tarojs/mini-runner": "3.5.4",
    "@tarojs/webpack-runner": "3.5.4",
    "@tarojs/webpack5-runner": "3.5.4",
    "@types/classnames": "^2.3.1",
    "@types/jasmine": "^4.0.3",
    "@types/lodash": "^4.14.182",
    "@types/moment": "^2.13.0",
    "@types/ramda": "^0.28.14",
    "@types/react": "^17.0.2",
    "@types/react-router-redux": "^5.0.21",
    "@types/redux-actions": "^2.6.2",
    "@types/reselect": "^2.2.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-preset-taro": "3.5.4",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.5.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.6.2",
    "react-native-linear-gradient": "^2.5.6",
    "stylelint": "9.3.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.1.0"
  }
}
