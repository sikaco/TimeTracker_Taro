{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
    "paths": {
      "$components": [
        "src/components/"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$pages": [
        "src/components/pages"
      ],
      "$pages/*": [
        "src/components/pages/*"
      ],
      "$xuder": [
        "src/components/xuder"
      ],
      "$xuder/*": [
        "src/components/xuder/*"
      ],
      "$utils": [
        "src/components/utils"
      ],
      "$utils/*": [
        "src/components/utils/*"
      ],
      "$service": [
        "src/service"
      ],
      "$service/*": [
        "src/service/*"
      ],
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}
